TestCase subclass: #AlgoChatTestsExtra	instanceVariableNames: ''	classVariableNames: ''	poolDictionaries: ''	category: 'TP1-Tests'!!AlgoChatTestsExtra methodsFor: 'all' stamp: 'neta 4/24/2017 12:15'!testNuevoChatGrupal	"comment stating purpose of message"	|miChatGrupal mensaje1 mensaje2 mensaje3 mensaje4 historial_chat|		miChatGrupal := ChatGrupal new.		mensaje1 := Mensaje new.	mensaje2 := Mensaje new.	mensaje3 := Mensaje new.	mensaje4 := Mensaje new.		mensaje1 establecerEmisor: 'Netanel'.	mensaje1 establecerReceptor: 'Los mejores'.	mensaje1 establecerContenido: 'Hola a todos, como estan???'.		miChatGrupal agregarMensaje: mensaje1.		mensaje2 establecerEmisor: 'Eliel'.	mensaje2 establecerReceptor: 'Los mejores'.	mensaje2 establecerContenido: 'Bien!!!!!!'.		miChatGrupal agregarMensaje: mensaje2.		mensaje3 establecerEmisor: 'Ilanit'.	mensaje3 establecerReceptor: 'Los mejores'.	mensaje3 establecerContenido: 'Shhh!!!!!!'.		miChatGrupal agregarMensaje: mensaje3.		mensaje4 establecerEmisor: 'Eliel'.	mensaje4 establecerReceptor: 'Los mejores'.	mensaje4 establecerContenido: 'Jajajajajajaja ;)'.		miChatGrupal agregarMensaje: mensaje4.		historial_chat := miChatGrupal obtenerConversacion.		self assert: (historial_chat at: 3)         equals: 'Netanel: Hola a todos, como estan???'.    self assert: (historial_chat at: 2)         equals: 'Eliel: Bien!!!!!!'.    self assert: (historial_chat at: 1)         equals: 'Ilanit: Shhh!!!!!!'.	self assert: (historial_chat at: 0)         equals: 'Eliel: Jajajajajajaja ;)'.		! !!AlgoChatTestsExtra methodsFor: 'all' stamp: 'neta 4/24/2017 11:03'!testNuevaAgendaConContactos	"comment stating purpose of message"	|miAgenda contacto1 contacto2 contacto3|		miAgenda := Agenda new.		self assert: miAgenda cantidadDeContactos         equals: 0.		contacto1 := Contacto new.	contacto1 establecerNombre:'Netanel'.	contacto2 := Contacto new.	contacto2 establecerNombre: 'Ilanit'.	contacto3 := Contacto new.	contacto3 establecerNombre: 'Eliel'. 		miAgenda agregarContacto: contacto1.	miAgenda agregarContacto: contacto2.	miAgenda agregarContacto: contacto3. 		self assert: miAgenda cantidadDeContactos         equals: 3.	! !!AlgoChatTestsExtra methodsFor: 'all' stamp: 'neta 4/24/2017 11:36'!testNuevoBuzonConMensajes	"comment stating purpose of message"	|miBuzon mensaje1 mensaje2 mensaje3 mensaje4 mensaje5|		miBuzon := Buzon new.		self assert: miBuzon cantidadTotalMensajes equals: 0.		mensaje1 := Mensaje new.	mensaje2 := Mensaje new.	mensaje3 := Mensaje new.	mensaje4 := Mensaje new.	mensaje5 := Mensaje new.		mensaje1 establecerEmisor: 'Netanel'.	mensaje1 establecerReceptor: 'Eliel'.	mensaje1 establecerContenido: 'Hola, como estas?'.		miBuzon archivarMensaje: mensaje1.		self assert: miBuzon cantidadTotalMensajes equals: 1.	self assert: (miBuzon cantidadMensajesDe: 'Netanel') equals: 1.	self assert: (miBuzon cantidadMensajesEnviadosA: 'Eliel') equals:1.		mensaje2 establecerEmisor: 'Ilanit'.	mensaje2 establecerReceptor: 'Eliel'.	mensaje2 establecerContenido: 'No me molestes'.		miBuzon archivarMensaje: mensaje2.		self assert: (miBuzon cantidadMensajesEnviadosA: 'Eliel') equals: 2.		mensaje3 establecerEmisor: 'Ilanit'.	mensaje3 establecerReceptor: 'Netanel'.	mensaje3 establecerContenido: 'No me molestes, quiero estudiar!!'.		miBuzon archivarMensaje: mensaje3.		mensaje4 establecerEmisor: 'Netanel'.	mensaje4 establecerReceptor: 'Eliel'.	mensaje4 establecerContenido: 'Hola ;)'.		miBuzon archivarMensaje: mensaje4.		mensaje5 establecerEmisor: 'Eliel'.	mensaje5 establecerReceptor: 'Netanel'.	mensaje5 establecerContenido: 'Chau!!'.		miBuzon archivarMensaje: mensaje5.		self assert: miBuzon cantidadTotalMensajes equals: 5.	self assert: (miBuzon cantidadMensajesEnviadosA: 'Eliel') equals: 3.	self assert: (miBuzon cantidadMensajesDe: 'Netanel') equals: 2.		miBuzon removerMensajesCon: 'Eliel'.		self assert: miBuzon cantidadTotalMensajes equals: 1.	self assert: (miBuzon cantidadMensajesDe: 'Eliel') equals: 0.	self assert: (miBuzon cantidadMensajesEnviadosA: 'Eliel') equals: 0.! !!AlgoChatTestsExtra methodsFor: 'all' stamp: 'neta 4/24/2017 12:00'!testNuevoConversacion	"comment stating purpose of message"		|miConversacion mensaje1 mensaje2 mensaje3 historial_chats|		miConversacion := Conversacion new.	miConversacion conversarCon: 'Eliel'.		self assert: (miConversacion conversoCon) equals: 'Eliel'.		mensaje1 := Mensaje new.	mensaje2 := Mensaje new.	mensaje3 := Mensaje new.		mensaje1 establecerEmisor: 'Eliel'.	mensaje1 establecerReceptor: 'Netanel'.	mensaje1 establecerContenido: 'Me aburro!!'.		miConversacion agregarMensaje: mensaje1.		mensaje2 establecerEmisor: 'Netanel'.	mensaje2 establecerReceptor: 'Eliel'.	mensaje2 establecerContenido: 'No me molestes!!'.		miConversacion agregarMensaje: mensaje2.		mensaje3 establecerEmisor: 'Eliel'.	mensaje3 establecerReceptor: 'Netanel'.	mensaje3 establecerContenido: 'Jajajaja ;)'.		miConversacion agregarMensaje: mensaje3.		historial_chats := miConversacion obtenerConversacion.		self assert: (historial_chats at: 0)         equals: 'Eliel: Jajajaja ;)'.    self assert: (historial_chats at: 1)         equals: 'Netanel: No me molestes!!'.    self assert: (historial_chats at: 2)         equals: 'Eliel: Me aburro!!'.	! !!AlgoChatTestsExtra methodsFor: 'all' stamp: 'neta 4/24/2017 11:44'!testNuevoGrupo	"comment stating purpose of message"	|miGrupo|		miGrupo := Grupo new.	miGrupo asignarNombre: 'Los mejores'.		self assert: miGrupo cantidadMiembrosEnGrupo equals: 1. "Me considero a yo mismo miembro del 	grupo"		miGrupo agregarContacto: 'Ilanit'.	miGrupo agregarContacto: 'Eliel.'.		self assert: miGrupo cantidadMiembrosEnGrupo equals: 3.! !